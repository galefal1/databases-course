# Amit Pinchas 200 RECS

-- ===============================================
--  IMDB_IJS recommendations seed script
-- ===============================================

USE imdb_ijs;  -- ensure correct schema

-- Drop existing tables if they exist
DROP TABLE IF EXISTS good_rec;
DROP TABLE IF EXISTS bad_rec;

-- Create GOOD_REC table
CREATE TABLE good_rec (
    pair_id INT PRIMARY KEY,
    source_movie VARCHAR(255),
    source_id INT,
    recommended_movie VARCHAR(255),
    rec_id INT,
    strength TINYINT,
    justification VARCHAR(500),
    suggested_by VARCHAR(100)
);

-- Create BAD_REC table with identical structure
CREATE TABLE bad_rec LIKE good_rec;

